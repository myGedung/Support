{
	"name": "mygedung/support",
	"description": "Support helpers.",
	"keywords": [
		"support",
		"helper",
		"laravel",
		"mygedung"
	],
	"license": "MIT",
	"require": {
		"php": ">=7.0.0"
	},
	"require-dev": {
        "illuminate/mail": "~5.5",
        "illuminate/validation": "~5.5",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.4",
        "symfony/translation": "~3.3"
	},
	"autoload": {
		"psr-4": {
			"myGedung\\Support\\": "src/"
		}
	},
	"extra": {
		"component": "package",
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
